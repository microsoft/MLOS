{
    "$schema": "https://raw.githubusercontent.com/microsoft/MLOS/main/mlos_bench/mlos_bench/config/schemas/cli/cli-schema.json",

    "config_path": [
        "mlos_bench/mlos_bench/config",
        "mlos_bench/mlos_bench/tests/config"
    ],

    "environment": "environments/composite/env-azure-ubuntu-redis.jsonc",
    "optimizer": "optimizers/one_shot_opt.jsonc",
    "storage": "storage/sqlite.jsonc",

    "random_init": true,
    "random_seed": 42,

    "tunable_values": [
        "tunable-values/tunable-values-example.jsonc"
    ],

    "globals": [
        "global_config.json"
    ],

    "experimentId": "RedisBench",
    "trialId": 1,

    "teardown": false,

    "log_file": "azure-redis-1shot.log",
    "log_level": "DEBUG"
}
