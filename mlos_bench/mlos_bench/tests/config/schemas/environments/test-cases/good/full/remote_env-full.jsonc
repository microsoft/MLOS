{
    "$schema": "https://raw.githubusercontent.com/microsoft/MLOS/main/mlos_bench/mlos_bench/config/schemas/environments/environment-schema.json",
    "name": "remote_env-full",
    "description": "More descriptive text.",
    "class": "mlos_bench.environments.remote.remote_env.RemoteEnv",
    "include_tunables": [
        "some/tunables.jsonc"
    ],
    "include_services": [
        "some/service.jsonc"
    ],
    "config": {
        "tunable_params": ["baz"],
        "required_args": ["foo"],
        "const_args": {
            "foo": "bar"
        },
        "wait_boot": true,
        "setup": [
            "/bin/bash -c true"
        ],
        "run": [
            "/bin/bash -c true"
        ],
        "teardown": [
            "/bin/bash -c true"
        ],

        "shell_params_match": [
            "^MLOS_[a-zA-Z0-9_]+$"
        ],
        "shell_params": [
            "foo"
        ],
        "shell_params_mapping": {
            "/some/invalid/env/name": "gets_mapped_to_this_env_var_name"
        }
    }
}
