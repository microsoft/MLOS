{
    "$schema": "https://raw.githubusercontent.com/microsoft/MLOS/main/mlos_bench/mlos_bench/config/schemas/environments/environment-schema.json",

    "name": "mock_env-full",
    "description": "More descriptive text.",
    "class": "mlos_bench.environments.mock_env.MockEnv",
    "include_services": [
        "path/to/some/service.jsonc"
    ],
    "include_tunables": [
        "path/to/some/tunables.jsonc"
    ],
    "config": {
        "tunable_params": [
            "some-tunable-params-name"
        ],
        "const_args": {
            "foo": "bar"
        },
        "required_args": [
            "foo"
        ],
        "mock_env_range": [0, 1],
        "mock_env_seed": 42,
        "mock_env_metrics": [
            "latency",
            "cost"
        ],
        "mock_trial_data": {
            "1": {
                "setup": {
                    "sleep": 0.1
                },
                "status": {
                    "metrics": {
                        "latency": 0.2,
                        "cost": 0.3
                    }
                },
                "run": {
                    "sleep": 0.2,
                    "status": "SUCCEEDED",
                    "metrics": {
                        "latency": 0.1,
                        "cost": 0.2
                    }
                },
                "teardown": {
                    "sleep": 0.1
                }
            },
            "2": {
                "setup": {
                    "exception": "Some exception"
                },
                "teardown": {
                    "exception": "Some other exception"
                }
            }
        }
    }
}
