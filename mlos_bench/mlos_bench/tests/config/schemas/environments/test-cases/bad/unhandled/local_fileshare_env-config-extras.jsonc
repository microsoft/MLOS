{
    "name": "local_fileshare_env-config-extras",
    "class": "mlos_bench.environments.LocalFileShareEnv",
    "config": {
        "extra": "not allowed",
        "run": [
            "/bin/bash -c true"
        ],
        "download": [
            {
                "from": "https://some-host/some-path/some-file.tar.gz",
                "to": "/some/local/path/some-file.tar.gz"
            }
        ]
    }
}
