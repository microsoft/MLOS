{
    "$schema": "https://raw.githubusercontent.com/microsoft/MLOS/main/mlos_bench/mlos_bench/config/schemas/services/service-schema.json",
    "description": "Azure SaaS Configuration Service",

    "class": "mlos_bench.services.remote.azure.AzureSaaSConfigService",

    "config": {
        "subscription": "subscription-id",
        "resourceGroup": "rg",
        "vmName": "vm-name",
        "provider": "Microsoft.DBforMySQL",
        "supportsBatchUpdate": true,
        "isFlex": true,
        "apiVersion": "2022-01-01",

        // 3 unhandled config parameters:
        "storageAccountName": "storage-account-name",
        "storageFileShareName": "file-share-name",

        "requestTimeout": 20
    }
}
