    "name": "Azure VM Ubuntu Redis",
    "class": "mlos_bench.environment.CompositeEnv",

    "include_tunables": [
        "global/tunables.jsonc"
    ],

    "include_services": [
        "services/remote/azure/service-provision-vm.jsonc",
        "services/remote/azure/service-fileshare.jsonc"
    ],

    "config": {
        "include_children": [
            "environment/vm/azure/provision-ubuntu-vm.jsonc",
            "environment/os/linux/boot/boot-ubuntu.jsonc",
            "environment/os/linux/runtime/setup-ubuntu.jsonc",
            "environment/apps/redis/redis.jsonc"
        ]
    }
}
