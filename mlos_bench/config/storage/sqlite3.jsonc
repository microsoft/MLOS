{
    "class": "mlos_bench.storage.SqlStorage",

    // Apply --config-path resolution to these config parameters:
    "resolve_path": ["database"],

    // Parameters must match kwargs of sqlalchemy.URL.create().
    "config": {
        "drivername": "sqlite",
        "database": "mlos_bench.sqlite3"
    }
}
